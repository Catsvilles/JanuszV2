{
	"name": "Boilerplate",
	"description": "Boilerplate: Node, Webpack, Isomorphic React, Less, Babel, Handlebars",
	"version": "1.0.0",
	"main": "./server.js",
	"author": "Fun Maker",
	"scripts": {
		"start": "rm -rf build && webpack --config webpack/server.dev.babel.js",
		"start:prod": "cd dist && npm start",
		"build:prod": "mkdir -p dist && npm-run-all build:prod-*",
		"build:prod-server": "webpack --config webpack/server.babel.js",
		"build:prod-client": "webpack --config webpack/client.babel.js",
		"build:prod-static": "cp -r web/static dist/ && cp -r sounds/sounds dist/",
		"build:prod-package": "babel-node webpack/package.babel.js"
	},
	"dependencies": {
		"@babel/plugin-proposal-class-properties": "^7.0.0-beta.51",
		"ansi-to-html": "^0.6.6",
		"axios": "^0.18.0",
		"body-parser": "latest",
		"chokidar": "latest",
		"colors": "latest",
		"compression": "^1.7.2",
		"cookie-parser": "^1.4.3",
		"detect-node": "latest",
		"discord.js": "^11.3.2",
		"express": "^4.16.3",
		"express-promise-router": "^3.0.2",
		"express-ws": "^3.0.0",
		"fluent-ffmpeg": "^2.1.2",
		"franc-min": "^4.0.0",
		"fs-extra": "^6.0.1",
		"http-status-codes": "^1.3.0",
		"intercept-stdout": "^0.1.2",
		"iso-639-3": "^1.1.0",
		"jsonwebtoken": "^8.3.0",
		"mobile-device-detect": "^0.2.3",
		"morgan": "^1.9.0",
		"mumble": "^0.3.15",
		"node-opus": "^0.3.0",
		"prop-types": "^15.6.2",
		"query-string": "latest",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-hot-loader": "^4.3.3",
		"react-router": "^4.3.1",
		"react-router-dom": "^4.3.1",
		"react-split-pane": "^0.1.77",
		"semantic-ui-react": "^0.80.2",
		"source-map-support": "^0.5.6",
		"uuid": "latest",
		"web-audio-engine": "^0.13.4",
		"youtube-dl": "^1.12.2"
	},
