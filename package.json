{
	"name": "Boilerplate",
	"description": "Boilerplate: Node, Webpack, Isomorphic React, Less, Babel, Handlebars",
	"version": "1.0.0",
	"main": "./server.js",
	"author": "Fun Maker",
	"scripts": {
		"start": "rm -rf build && webpack --config webpack/server.dev.babel.js",
		"start:prod": "cd dist && npm start",
		"build:prod": "mkdir -p dist && npm-run-all build:prod-*",
		"build:prod-server": "webpack --config webpack/server.babel.js",
		"build:prod-client": "webpack --config webpack/client.babel.js",
		"build:prod-static": "cp -r web/static dist/ && cp -r sounds/sounds dist/",
		"build:prod-package": "babel-node webpack/package.babel.js"
	},
	"dependencies": {
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"ansi-to-html": "^0.6.11",
		"axios": "^0.19.0",
		"bezier-easing": "^2.1.0",
		"body-parser": "latest",
		"chokidar": "latest",
		"colors": "latest",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.4",
		"detect-node": "latest",
		"discord.js": "^11.5.1",
		"express": "^4.17.1",
		"express-promise-router": "^3.0.3",
		"express-ws": "^4.0.0",
		"fluent-ffmpeg": "^2.1.2",
		"franc-min": "^4.1.0",
		"fs-extra": "^8.0.1",
		"http-status-codes": "^1.3.2",
		"intercept-stdout": "^0.1.2",
		"iso-639-3": "^1.2.0",
		"jsonwebtoken": "^8.5.1",
		"mobile-device-detect": "^0.2.3",
		"morgan": "^1.9.1",
		"mumble": "^0.3.20",
		"node-opus": "^0.3.2",
		"prop-types": "^15.7.2",
		"query-string": "latest",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-hot-loader": "^4.11.1",
		"react-router": "^5.0.1",
		"react-router-dom": "^5.0.1",
		"react-split-pane": "^0.1.87",
		"semantic-ui-react": "^0.87.2",
		"source-map-support": "^0.5.12",
